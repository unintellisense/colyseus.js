{
  "name": "colyseus.js",
  "version": "0.6.0",
  "description": "Multiplayer Game Client for the Browser",
  "keywords": [
    "multiplayer",
    "mmo",
    "networking",
    "websockets"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/gamestdio/colyseus.js.git"
  },
  "main": "lib/index.js",
