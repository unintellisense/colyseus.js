  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/fast-json-patch": "^1.0.4",
    "@types/fossil-delta": "^0.2.1",
    "@types/msgpack-lite": "^0.1.1",
    "chai": "^3.5.0",
    "colyseus": "^0.4.0",
    "express": "^4.13.3",
    "mocha": "^3.1.0",
    "nodemon": "^1.7.1",
    "ts-loader": "^2.0.0",
